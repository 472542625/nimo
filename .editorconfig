# http://editorconfig.org
root = true

## code UTF-8;
## delete line right space;
## end line add one line;
[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8

[*.{js,html,css}]
indent_style = space
indent_size = 4

[*.{json,yml}]
indent_style = space
indent_size = 2
